@model IEnumerable<EventApp.Models.Tblcity>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tblstate.Statename)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cityname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tblstate.Statename)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cityname)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Cityid }) |
            @Html.ActionLink("Details", "Details", new { id=item.Cityid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Cityid })
        </td>
    </tr>
}

</table>
