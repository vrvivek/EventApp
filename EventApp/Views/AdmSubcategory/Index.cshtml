@model IEnumerable<EventApp.Models.Tblsubcategory>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tblcategory.Categoryname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subcategoryname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tblcategory.Categoryname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Subcategoryname)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Subcategoryid }) |
            @Html.ActionLink("Details", "Details", new { id=item.Subcategoryid })
        </td>
    </tr>
}

</table>
